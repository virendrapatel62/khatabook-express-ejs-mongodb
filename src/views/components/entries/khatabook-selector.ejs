<div class="container">
  <h1>Select A Khatabook To Make An Entry</h1>
  <hr />

  <ul class="list-group">
    <% for(let khatabook of locals.khatabooks || []){ %>
    <li class="list-group-item">
      <a
        style="text-decoration: none"
        class="text-dark"
        href="/entries/khatabook/<%= khatabook.id %>"
        ><%= khatabook.title %></a
      >
    </li>
    <% } %>
  </ul>
</div>
